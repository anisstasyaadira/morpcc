<!DOCTYPE html>
<html>

<metal:master use-macro="load_template('master/page.pt').macros.master">
    <metal:slot fill-slot="page-title">
        Reference Data
    </metal:slot>
    <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="${request.relative_url('/+site-settings')}">Settings</a></li>
        <li class="breadcrumb-item"><a href="${request.relative_url('/dictionaryentity/+listing')}">Manage Data Dictionary</a></li>
        <li class="breadcrumb-item active" aria-current="page">View Reference Data</li>
    </ul>
    
    <metal:slot fill-slot="page-body">
        <div id="refdatamgr">
            <div class="col-md-7 col-sm-12 col-xs-12">
                <div class="row">
                    <div metal:use-macro="load_template('master/panel.pt').macros.panel">
                        <h2 metal:fill-slot="panel-title">View Reference Data</h2>
                        <div metal:fill-slot="panel-content">
                            <div class="form-group">
                                <label class="col-sm-12 col-md-12 col-lg-4">Name</label>
                                <div tal:content="context.model['name']"></div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-12 col-md-12 col-lg-4">Title</label>
                                <div tal:content="context.model['title']"></div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-12 col-md-12 col-lg-4">Description</label>
                                <div tal:content="context.model['description']"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div metal:use-macro="load_template('master/panel.pt').macros.panel">
                        <h2 metal:fill-slot="panel-title">Key and Properties</h2>
                        <div metal:fill-slot="panel-content">
                            <!-- Here goes the ref data key and properties -->
                            <div class="col-md-4">
                                <div id="refdatakey-wrapper">
                                    <h2>Keys</h2>
                                    <table id="refdatakey-listing" class="table table-striped table-bordered nowrap"
                                        cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>0</td>
                                            </tr>
                                            <tr>
                                                <td>1</td>
                                            </tr>
                                            <tr>
                                                <td>2</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="col-md-8">
                                <div id="refdataprop-wrapper">
                                    <h2>Properties</h2>
                                    <table id="refdataprop-listing" class="table table-striped table-bordered dt-responsive nowrap"
                                        cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th>Property</th>
                                                <th>Value</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Label</td>
                                                <td>correspondence address</td>
                                            </tr>
                                            <tr>
                                                <td>Label</td>
                                                <td>residential address</td>
                                            </tr>
                                            <tr>
                                                <td>Label</td>
                                                <td>office address</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-5 col-sm-12 col-xs-12">
                <div metal:use-macro="load_template('master/panel.pt').macros.panel">
                    <h2 metal:fill-slot="panel-title">Metadata</h2>
                    <div metal:fill-slot="panel-content">
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">id</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['id']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">uuid</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['uuid']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">creator</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['creator']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">created</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['created']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">modified</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['modified']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">state</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['state']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">deleted</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['deleted']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">blobs</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['blobs']"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-12 col-md-12 col-lg-4">xattrs</label>
                                <div class="col-sm-12 col-md-12 col-lg-8" tal:content="context.model['xattrs']"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </metal:slot>
</metal:master>
</html>